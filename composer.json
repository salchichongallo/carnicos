{
    "name": "salchichongallo/carnicos",
    "description": "Caso de estudio Programaci√≥n para la Web con PHP",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "illuminate/container": "^5.6",
        "illuminate/database": "^5.6",
        "illuminate/pipeline": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
      "psr-4": {
        "App\\": "app/",
        "Itm\\": "src/Itm/",
        "Meat\\": "src/Meat/"
      },
      "files": [
        "./app/helpers.php"
      ]
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "config": {
      "preferred-install": "dist",
      "sort-packages": true,
      "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
